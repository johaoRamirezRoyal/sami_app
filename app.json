{
  "expo": {
    "name": "SAMI.app",
    "slug": "SAMI.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icono.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icono.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.SAMI.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "././assets/icono.jpg"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "ALLOW $(PRODUCT_NAME) TO ACCESS THE CAMERA",
          "microphonePermission": "ALLOW $(PRODUCT_NAME) TO ACCESS THE MICROPHONE",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f6c19fe-f363-4877-bd4d-0d8fef27d7aa"
      }
    }
  }
}
